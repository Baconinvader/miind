<Simulation>
<WeightType>DelayedConnection</WeightType>
<Algorithms>
<Algorithm type="MeshAlgorithm" name="E" modelfile="cond.model" >
<TimeStep>0.00001</TimeStep>
<MatrixFile>cond_0_0.05_0.mat</MatrixFile>
</Algorithm>
<Algorithm type="RateFunctor" name="Exc Input">
<expression>1000.</expression>
</Algorithm>
</Algorithms>
<Nodes>
<Node algorithm="E" name="Cond E" type="EXCITATORY_DIRECT" />
<Node algorithm="Exc Input" name="Inp" type="NEUTRAL" />
</Nodes>
<Connections>
<Connection In="Inp" Out="Cond E">1   0.05 0</Connection>
</Connections>
<Reporting>
<Density node="Cond E" t_start="0.0" t_end="0.1" t_interval="0.001" />
