<Rate node="Cond E" t_interval="0.001"/>
</Reporting>
<SimulationIO>
<SimulationName>cond.dat</SimulationName>
<OnScreen>FALSE</OnScreen>
<WithState>TRUE</WithState>
<WriteNet>FALSE</WriteNet>
<CanvasParameter>
<T_min>0</T_min>
<T_max>0.3</T_max>
