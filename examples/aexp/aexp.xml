<Simulation>
<WeightType>DelayedConnection</WeightType>
<Variable Name="omega">1.0</Variable>
<Algorithms>
<Algorithm type="MeshAlgorithm" name="LIF_excitatory Algorithm" modelfile="aexp.model" >
<TimeStep>0.00001</TimeStep>
<!--<MatrixFile>aexp_-1_0_0.mat</MatrixFile>-->
<MatrixFile>aexp_1_0_0.mat</MatrixFile>
</Algorithm>
<Algorithm type="RateFunctor" name="Exc Input">
<expression>2000.</expression>
</Algorithm>
<Algorithm type="RateFunctor" name="Inh Input">
<expression><![CDATA[t < 0.1 ? 0. : 0.]]></expression>
</Algorithm>
</Algorithms>
<Nodes>
<Node algorithm="LIF_excitatory Algorithm" name="LIFE" type="EXCITATORY_DIRECT" />
<Node algorithm="Exc Input" name="Excit" type="NEUTRAL" />
<Node algorithm="Inh Input" name="Inhib" type="NEUTRAL" />
</Nodes>
<Connections>
<Connection In="Excit" Out="LIFE">1  1. 0</Connection>
<!--<Connection In="Inhib" Out="LIFE">1 -1. 0</Connection>-->
</Connections>
<Reporting>
<Density node="LIFE" t_start="0.0" t_end="1.0" t_interval="0.001" />
<!--<Display node="LIFE" />-->
<Rate node="LIFE" t_interval="0.001"/>
</Reporting>
<SimulationIO>
<SimulationName>single</SimulationName>
<OnScreen>FALSE</OnScreen>
<WithState>TRUE</WithState>
<WriteNet>FALSE</WriteNet>
<CanvasParameter>
<T_min>0</T_min>
<T_max>0.3</T_max>
<F_min>0</F_min>
<F_max>20</F_max>
<State_min>0</State_min>
<State_max>1.0</State_max>
<Dense_min>0</Dense_min>
<Dense_max>2.5</Dense_max>
</CanvasParameter>
<CanvasNode Name="LIFE" />
</SimulationIO>
<SimulationRunParameter>
<max_iter>1000000</max_iter>
<t_begin>0</t_begin>
<t_end>1.0</t_end>
<t_report>0.013</t_report>
<t_state_report>1e-03</t_state_report>
<t_step>0.0001</t_step>
<t_update>0.01</t_update>
<name_log>single.log</name_log>
</SimulationRunParameter>
</Simulation>
